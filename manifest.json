{
	"manifest_version": 2,
  	"name": "Productiva",
  	"version": "0.1",
  	"chrome_url_overrides" : {
  		"newtab" : "home.html"
  	},
	"browser_action": {
    "default_icon": "icon.png"
	},
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
	"permissions": ["bookmarks"]
}